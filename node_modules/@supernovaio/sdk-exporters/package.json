{
    "name": "@supernovaio/sdk-exporters",
    "version": "2.0.4",
    "description": "Supernova.io Exporter SDK",
    "main": "./build/supernova-sdk-typescript.js",
    "types": "./build/sdk-typescript/src/index.d.ts",
    "license": "MIT",
    "keywords": [
        "Supernova",
        "Design Systems",
        "Supernovaio",
        "SDK",
        "Design Tokens",
        "Tokens",
        "Assets",
        "Components",
        "Documentation",
        "Markdown",
        "CMS"
    ],
    "files": [
        "build"
    ],
    "scripts": {
        "clean": "trash build test",
        "build": "npm run build:dev",
        "build:dev": "npm run clean && webpack --mode development --config ./webpack.config.sdk-exporters.js",
        "build:production": "npm run clean && webpack --mode production --config ./webpack.config.sdk-exporters.js",
        "publish-package-sdk": "npm run clean && sh ./scripts/publish-sdk.sh",
        "publish-package-sdk-exporters": "npm run clean && sh ./scripts/publish-sdk-exporters.sh",
        "publish-packages": "sh ./scripts/publish-packages.sh"
    },
    "scripts-info": {
        "build:types": "Clean and rebuild the project",
        "publish-package-sdk": "Publish SDK package to NPM",
        "publish-package-pulsar": "Publish Pulsar type package to NPM",
        "publish-packages": "Publish both SDK and Pulsar package to NPM"
    },
    "engines": {
        "node": ">=8.9"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^14.14.6",
        "dotenv": "^16.0.0",
        "npm-run-all": "4.1.5",
        "trash-cli": "^5.0.0",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.9.1",
        "tsconfig-paths-webpack-plugin": "^3.5.2",
        "tslint": "^5.18.0",
        "typescript": "^4.1.5",
        "webpack": "^5.66.0",
        "webpack-cli": "^4.10.0",
        "webpack-node-externals": "^3.0.0"
    },
    "config": {}
}